<Window x:Class="Tigers.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tigers"
        xmlns:viewModel="clr-namespace:Tigers.ViewModel"
        xmlns:converter="clr-namespace:Tigers.ViewModel.Converter"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <viewModel:ViewModel/>
    </Window.DataContext>
    <Window.Resources>
        <converter:Converter x:Key="Converter"/>
        <converter:MultiConverter x:Key="MultiConverter"/>
    </Window.Resources>
    <Grid>
        
    </Grid>
</Window>
